<?xml version="1.0" encoding="utf-8"?>
<VectorNetServer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Strings>
    <HostedBy />
    <MOTD />
  </Strings>
  <Network>
    <ListenPort>4800</ListenPort>
    <ReadBufferSize>1024</ReadBufferSize>
    <SendBufferInterval>200</SendBufferInterval>
    <MaxConnectionsBacklog>20</MaxConnectionsBacklog>
    <MaxClientSendBacklog>30</MaxClientSendBacklog>
    <ClientCloseWait>2000</ClientCloseWait>
  </Network>
  <DefaultChannels>
    <Main>Main</Main>
    <Admin>Backstage</Admin>
    <Void>The Void</Void>
  </DefaultChannels>
  <NamingRules>
    <UsernameMinLength>2</UsernameMinLength>
    <UsernameMaxLength>14</UsernameMaxLength>
    <UsernameRegex>^([a-zA-Z0-9]+?[\._\-]{0,1})*?$</UsernameRegex>
  </NamingRules>
  <Timers>
    <CheckInterval>1000</CheckInterval>
    <GarbageInterval>5000</GarbageInterval>
    <FloodDecrementInterval>1000</FloodDecrementInterval>
  </Timers>
  <FloodConditions>
    <UserConnectionThreshold>2</UserConnectionThreshold>
  </FloodConditions>
  <DatabaseFilename>vnet.sqlite</DatabaseFilename>
  <ChannelOwnerTimeout>15</ChannelOwnerTimeout>
  <AllowAES>true</AllowAES>
  <UseIdleSystem>false</UseIdleSystem>
  <MaxUsersPerIP>10</MaxUsersPerIP>
  <MaxUsersPerChannel>50</MaxUsersPerChannel>
</VectorNetServer>